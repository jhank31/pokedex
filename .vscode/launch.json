{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter: Run All Tests",
      "type": "dart",
      "request": "launch",
      "program": "test/",
      "flutterMode": "debug"
    },
    {
      "name": "Flutter: Run Current Test File",
      "type": "dart",
      "request": "launch",
      "program": "${file}",
      "flutterMode": "debug"
    },
    {
      "name": "Flutter: Development",
      "type": "dart",
      "request": "launch",
      "program": "lib/main_development.dart",
      "args": ["--flavor", "development"]
    },
    {
      "name": "Flutter: Production",
      "type": "dart",
      "request": "launch",
      "program": "lib/main_production.dart",
      "args": ["--flavor", "production"]
    },
    {
      "name": "Flutter: Profile",
      "type": "dart",
      "request": "launch",
      "program": "lib/main_staging.dart",
      "args": ["--flavor", "staging", "--profile"]
    }
  ]
}
